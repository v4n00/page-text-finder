{
	"manifest_version": 3,
	"name": "TextFinder",
	"version": "1.0",
	"permissions": ["activeTab", "storage"],
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js",
		"type": "module"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
