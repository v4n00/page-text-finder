{
	"manifest_version": 3,
	"name": "PageTextFinder",
	"description": "Find text on the page, based on your input.",
	"version": "1.1.0",
	"permissions": ["storage", "unlimitedStorage", "activeTab"],
	"icons": {
		"128": "icon128.png"
	},
	"action": {
		"default_popup": "popup.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["content.js"]
		}
	]
}
